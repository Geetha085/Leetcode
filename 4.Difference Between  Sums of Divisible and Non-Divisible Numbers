def difference(m, n):
    sum_divisible = 0
    sum_not_divisible = 0
    for i in range(1, m+1):
        if i % n == 0:  # Check if the number is divisible by n
            sum_divisible += i
        else:
            sum_not_divisible += i
    print(sum_not_divisible - sum_divisible)
difference(20,4)
